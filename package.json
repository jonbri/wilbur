{
  "name": "wilbur",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "mkdir": "mkdir -p test-results playwright-report .nyc_output",
    "clean": "yarn mkdir && rm -Rf test-results/* playwright-report/* .nyc_output/*",
    "clean:show": "yarn mkdir && tree test-results playwright-report .nyc_output",
    "dev": "vite --host",
    "start": "export USE_BABEL_PLUGIN_ISTANBUL=1 && vite --host",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "test": "mkdir -p .nyc_output && npx playwright test",
    "test:interactive": "mkdir -p .nyc_output && npx playwright test --ui-host=0.0.0.0 --ui-port=8080",
    "test:report": "mkdir -p .nyc_output && npx nyc report --reporter=html",
    "docker-run": "./bin/docker-run",
    "docker-stop": "./bin/docker-stop",
    "docker-enter": "./bin/docker-enter",
    "report": "npx nyc report --reporter=html",
    "report:html": "npx nyc report --reporter=html",
    "report:lcov": "npx nyc report --reporter=lcov",
    "report:text": "npx nyc report --reporter=text",
    "report:serve": "cd coverage && serve"
  },
  "dependencies": {
    "@sillylib/sillylib": "^1.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.39.0",
    "@types/node": "^20.8.10",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.3",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "serve": "^14.2.1",
    "typescript": "^5.0.2",
    "vite": "^4.4.5",
    "vite-plugin-istanbul": "^5.0.0"
  },
  "license": "ISC"
}
