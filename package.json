{
  "name": "wilbur",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "start": "vite --host",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "test": "mkdir -p .nyc_output && npx playwright test",
    "test:interactive": "mkdir -p .nyc_output && npx playwright test --ui-host=0.0.0.0 --ui-port=8080",
    "test:report": "mkdir -p .nyc_output && npx nyc report --reporter=html",
    "docker-run": "./bin/docker-run",
    "docker-stop": "./bin/docker-stop",
    "docker-enter": "./bin/docker-enter"
  },
  "dependencies": {
    "@sillylib/sillylib": "^1.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "v8-to-istanbul": "^9.1.3"
  },
  "devDependencies": {
    "@playwright/test": "^1.39.0",
    "@types/node": "^20.8.10",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.3",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "typescript": "^5.0.2",
    "vite": "^4.4.5"
  },
  "license": "ISC"
}
